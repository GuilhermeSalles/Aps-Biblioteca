            <Insets bottom="5.0" left="5.0" top="5.0" />
         </padding>
      </ToolBar>
      <TableView prefHeight="321.0" prefWidth="566.0">
        <columns>
          <TableColumn prefWidth="75.0" text="ISBN" />
          <TableColumn prefWidth="300.0" text="Título" />
